# Camera Configuration
# Format: rtsp://username:password@hostname:port/path
# The path should include /ch0 (main/high quality) or /ch1 (sub/lower quality)
# You can toggle between streams in the web interface

CAM_1=rtsp://thingino:thingino@sonoff-cam-1.local:554/ch0
CAM_2=rtsp://thingino:thingino@sonoff-cam-2.local:554/ch0
CAM_3=rtsp://thingino:thingino@sonoff-cam-3.local:554/ch0

# Camera Names (optional, for display)
CAM_1_NAME=Camera 1
CAM_2_NAME=Camera 2
CAM_3_NAME=Camera 3

# Flask Configuration
FLASK_SECRET_KEY=change-this-to-random-secret-key
FLASK_PORT=8080
FLASK_DEBUG=True

# Stream Settings
JPEG_QUALITY=80
RETRY_INTERVAL=5
STREAM_TIMEOUT=10
